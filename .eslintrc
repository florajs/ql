{
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "block-scoped-var": 2,
    "brace-style": 0,
    "consistent-this": [0, "self"],
    "curly": [2, "multi-line"],
    "complexity": [0, 11],
    "dot-notation": 0,
    "indent": 4,
    "max-depth": [2, 6],
    "max-len": [1, 120, 4],
    "max-nested-callbacks": [2, 5],
    "max-params": [2, 4],
    "no-empty": 0,
    "no-loop-func": 0,
    "no-multi-spaces": 0,
    "no-trailing-spaces": 0,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "quotes": [2, "single"],
    "radix": 2,
    "space-infix-ops": 0,
    "space-unary-ops": 0,
    "strict": 2,
    "valid-jsdoc": 0
  }
}
